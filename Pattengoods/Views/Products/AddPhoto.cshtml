@{
  Layout = "_Layout";
}

@model Pattengoods.Models.Product

<form
  asp-action="UploadPhoto"
  asp-controller="Images"
  method="post"
  enctype="multipart/form-data"
>
  @Html.HiddenFor(p => p.ProductId)

  <input
    type="file"
    id="file1"
    name="file1"
    multiple="multiple"
  />

  <button type="submit" class="btn btn-primary">Upload Photo</button>
</form>